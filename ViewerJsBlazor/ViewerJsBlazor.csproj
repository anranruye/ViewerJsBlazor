<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFrameworks>net5;net6</TargetFrameworks>
    <RazorLangVersion>3.0</RazorLangVersion>
    <Authors>hao sun</Authors>
    <Description>A blazor wrapper for Viewer.js JavaScript image viewer library.</Description>
    <Copyright>hao sun</Copyright>
    <RepositoryType>git</RepositoryType>
    <PackageTags>blazor;viewerjs;viewer.js;image viewer;image;razor;component</PackageTags>
    <PackageProjectUrl>https://github.com/anranruye/ViewerJsBlazor</PackageProjectUrl>
    <RepositoryUrl>https://github.com/anranruye/ViewerJsBlazor</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="wwwroot\Element.cs" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components" Version="5.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0" />
  </ItemGroup>

</Project>
